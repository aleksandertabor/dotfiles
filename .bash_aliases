alias sail='./vendor/bin/sail'
alias rector='vendor/bin/rector'
alias a="php artisan"
alias c="composer"
alias larastan="vendor/bin/phpstan analyse"
alias larastancc="vendor/bin/phpstan clear-result-cache"
alias glog="git log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit"
alias hosts="code /etc/hosts"
alias phpversions="update-alternatives --list php"
alias changephp="update-alternatives --config php"
alias nah="git reset --hard; git clean -df"
alias cshow="composer show"
alias cshowt="composer show -t"
alias cout="composer outdated"
alias bat="batcat"
alias testdox="./vendor/bin/phpunit --testdox"

alias sudo='sudo '
